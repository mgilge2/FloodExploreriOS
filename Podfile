# Uncomment the next line to define a global platform for your project
 platform :ios, '10'

source 'https://github.com/CocoaPods/Specs.git'
target 'FloodExplorer' do
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'XLPagerTabStrip'
  pod 'moa', '~> 9.0'
  pod 'Auk', '~> 8.0'
  pod 'AZDropdownMenu'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FloodExplorer

  target 'FloodExplorerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FloodExplorerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
